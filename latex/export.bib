	keywords = {bed\_mobility, bed\_rise, chair\_rise, physical\_therapy\_functional\_horizontal\_activity\_intervention, transfers},
	month = {May},
	number = {5},
	pages = {526--533},
	posted-at = {2009-06-16 16:46:14},
	priority = {2},
	title = {Chair and bed rise performance in ADL-impaired congregate housing residents.},
	url = {http://view.ncbi.nlm.nih.gov/pubmed/10811546},
	volume = {48},
	year = {2000}
}




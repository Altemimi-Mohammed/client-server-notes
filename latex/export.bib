	keywords = {hodgkin, lymphoma, spleen, ultrasound},
	month = {May},
	number = {2},
	pages = {574--582},
	posted-at = {2009-06-16 14:30:51},
	priority = {2},
	title = {Contrast-enhanced harmonic compound US of the spleen to increase staging accuracy in patients with Hodgkin lymphoma: a prospective study.},

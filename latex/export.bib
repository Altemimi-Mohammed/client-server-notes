	isbn = {0-7695-3047-8},
	pages = {369--380},
	posted-at = {2009-06-16 18:54:11},
	priority = {2},
	publisher = {IEEE Computer Society},
	title = {Data Access Partitioning for Fine-grain Parallelism on Multicore Architectures},
	url = {http://dx.doi.org/10.1109/MICRO.2007.11},
	year = {2007}
}




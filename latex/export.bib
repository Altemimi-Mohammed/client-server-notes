@article{citeulike:4870632,
	abstract = {Limited time period running trends are created from various microwave sounding unit (MSU) difference time series between the
                     University of Alabama in Huntsville and Remote Sensing System (RSS) group's lower troposphere (LT) and mid troposphere to
                     lower stratosphere channels. This is accomplished in an effort to determine the causes of the greatest discrepancies between

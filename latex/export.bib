	journal = {Journal of Discrete Algorithms},
	keywords = {bsp, dbsp, models, parallel},
	month = {April},
	number = {2},
	pages = {151--166},
	posted-at = {2009-06-16 17:28:06},
	priority = {2},
	title = {Optimal broadcast on parallel locality models},
	url = {http://dx.doi.org/10.1016/S1570-8667(03)00023-6},

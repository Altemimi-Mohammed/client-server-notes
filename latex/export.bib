	priority = {2},
	title = {Genetic screening methods for the detection of mutations responsible for multiple endocrine neoplasia type 1},
	url = {http://dx.doi.org/10.1016/j.ymgme.2004.08.013},
	volume = {83},
	year = {2004}
}




	issn = {1097-0290},
	journal = {Biotechnology and bioengineering},
	keywords = {coc},
	month = {October},
	number = {2},
	pages = {263--272},
	posted-at = {2009-06-16 15:25:43},
	priority = {2},
	title = {Directed evolution of LuxI for enhanced OHHL production.},
	url = {http://dx.doi.org/10.1002/bit.21901},
	volume = {101},
	year = {2008}
}




	isbn = {0-7695-0170-2},
	location = {Atlanta, Georgia, United States},
	pages = {4--15},
	posted-at = {2009-06-16 19:11:40},
	priority = {2},
	publisher = {IEEE Computer Society},
	title = {Maps: a compiler-managed memory system for raw machines},
	url = {http://dx.doi.org/10.1145/300979.300980},
	year = {1999}
}




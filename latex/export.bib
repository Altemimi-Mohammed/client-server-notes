	issn = {0002-8614},
	journal = {Journal of the American Geriatrics Society},
	keywords = {bed\_mobility, physical\_therapy\_neuro\_functional\_horizontal\_activity\_intervention, task\_specific\_training, transfers},
	month = {November},
	number = {11},
	pages = {1418--1427},
	posted-at = {2009-06-16 16:40:09},
	priority = {2},
	title = {Task-specific resistance training to improve the ability of activities of daily living-impaired older adults to rise from a bed and from a chair.},

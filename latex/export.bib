	priority = {3},
	title = {Pessimistic, Anxious, and Depressive Personality Traits Predict All-Cause Mortality: The Mayo Clinic Cohort Study of Personality and Aging},
	url = {http://dx.doi.org/10.1097/PSY.0b013e31819e67db},

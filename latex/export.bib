	journal = {Bioinformatics},
	keywords = {meta-analysis, mutual\_information},
	month = {January},
	pages = {btp007+},
	posted-at = {2009-06-16 16:32:13},
	priority = {1},
	title = {A novel meta-analysis method exploiting consistency of high-throughput experiments},
	url = {http://dx.doi.org/10.1093/bioinformatics/btp007},
	year = {2009}
}




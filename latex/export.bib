	posted-at = {2009-06-16 16:26:43},
	priority = {2},
	title = {Crystal structure of the heme-IsdC complex, the central conduit of the Isd iron/heme uptake system in Staphylococcus aureus.},
	url = {http://dx.doi.org/10.1074/jbc.M700234200},
	volume = {282},
	year = {2007}
}




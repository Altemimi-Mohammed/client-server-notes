	keywords = {coc},
	month = {May},
	number = {18},
	pages = {7397--7401},
	posted-at = {2009-06-16 14:50:01},
	priority = {2},
	title = {Following evolution's lead to a single residue switch for diterpene synthase product outcome},
	url = {http://dx.doi.org/10.1073/pnas.0611454104},
	volume = {104},
	year = {2007}
}




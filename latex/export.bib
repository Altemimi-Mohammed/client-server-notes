	issn = {1528-1175},
	journal = {Anesthesiology},
	month = {June},
	posted-at = {2009-06-16 10:43:29},
	priority = {2},
	title = {Attenuation of Mechanical Hypersensitivity by an Antagonist of the TRPA1 Ion Channel in Diabetic Animals.},
	url = {http://dx.doi.org/10.1097/ALN.0b013e3181a1642b},
	year = {2009}
}




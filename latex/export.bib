	posted-at = {2009-06-16 12:56:56},
	priority = {2},
	title = {NMR multiple quantum coherences in quasi-one-dimensional spin systems: Comparison with ideal spin-chain dynamics},
	url = {http://arxiv.org/abs/0906.2434},
	year = {2009}
}




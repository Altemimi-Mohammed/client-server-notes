	keywords = {fdg, infection, pet},
	month = {March},
	number = {1},
	pages = {17--29},
	posted-at = {2009-06-16 14:41:52},
	priority = {2},
	title = {Imaging of infectious diseases using [18F] fluorodeoxyglucose PET.},
	url = {http://view.ncbi.nlm.nih.gov/pubmed/17657204},

	posted-at = {2009-06-16 14:49:02},
	priority = {2},
	title = {Diagnostic accuracy of serial CT/magnetic resonance imaging review vs. positron emission tomography/CT in colorectal cancer patients with suspected and known recurrence.},
	url = {http://dx.doi.org/10.1007/DCR.0b013e31819d11e6},
	volume = {52},

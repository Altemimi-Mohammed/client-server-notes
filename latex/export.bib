	posted-at = {2009-06-16 18:42:05},
	priority = {2},
	title = {Ranking the AR4 climate models over the Murray-Darling Basin using simulated maximum temperature, minimum temperature and precipitation},
	url = {http://dx.doi.org/10.1002/joc.1612},
	volume = {28},

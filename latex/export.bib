	posted-at = {2009-06-16 10:03:02},
	priority = {2},
	title = {Clinical laboratory evaluation of the syndrome of inappropriate secretion of antidiuretic hormone.},
	url = {http://dx.doi.org/10.2215/CJN.04431007},

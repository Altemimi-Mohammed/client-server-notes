	keywords = {neuroscience},
	month = {May},
	posted-at = {2009-06-16 15:40:44},
	priority = {2},
	publisher = {Nature Publishing Group},
	title = {Two types of dopamine neuron distinctly convey positive and negative motivational signals},
	url = {http://dx.doi.org/10.1038/nature08028},
	year = {2009}
}




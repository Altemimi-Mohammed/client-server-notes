	issn = {0022-2836},
	journal = {Journal of molecular biology},
	keywords = {ras},
	month = {January},
	number = {3},
	pages = {435--446},
	posted-at = {2009-06-16 17:17:40},
	priority = {2},
	title = {Solution structure and functional analysis of the cysteine-rich C1 domain of kinase suppressor of Ras (KSR).},
	url = {http://dx.doi.org/10.1006/jmbi.2001.5263},
	volume = {315},
	year = {2002}
}




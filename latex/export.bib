	journal = {Developmental Biology},
	keywords = {devo, evo, eye, neurons},
	month = {May},
	posted-at = {2009-06-16 12:09:16},
	priority = {2},
	title = {Eye evolution at high resolution: The neuron as a unit of homology},
	url = {http://dx.doi.org/10.1016/j.ydbio.2009.05.565},
	year = {2009}
}




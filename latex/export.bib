	doi = {10.1175/1520-0469(2004)061\%3C0715:AAOTPO\%3E2.0.CO;2},
	journal = {Journal of the Atmospheric Sciences},
	keywords = {bias, climate, cloud, error, model, parameterization, radiation, radiative-transfer},
	month = {March},
	number = {6},
	pages = {715--732},
	posted-at = {2009-06-16 15:51:25},
	priority = {2},
	title = {An Assessment of the Parameterization of Subgrid-Scale Cloud Effects on Radiative Transfer. Part I: Vertical Overlap},
	url = {http://dx.doi.org/10.1175/1520-0469(2004)061\%3C0715:AAOTPO\%3E2.0.CO;2},
	volume = {61},
